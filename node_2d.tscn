[sub_resource type="GDScript" id="GDScript_e8dv2"]
resource_name = "Wizard"
script/source = "extends CharacterBody2D

var lastbounce
const SPEED = 1800.0
const JUMP_VELOCITY = -400.0
var facing = 1
var coyote_time = 0
var sliding = -1
var health = 100
var checkpoint = Vector2(224, 384)
var max_health = 100
var state = \"alive\"
func _ready() -> void:
	$AnimationPlayer.play(\"spawn\")

func _physics_process(delta: float) -> void:
	# Make the physics tick rate equal to the frame rate
	if Engine.get_frames_per_second() >= 10:
		Engine.physics_ticks_per_second = Engine.get_frames_per_second()
	
	# Add the gravity.
	$\"../../CanvasLayer/HealthBar/HealthLabel\".text = \"Health: \" + str(health)
	if state == \"alive\":
		var direction := Input.get_axis(\"left\", \"right\")
		if health <= 0:
			health = 0
		if position.y > 1000:
			position = checkpoint
		coyote_time -= 0.5 * delta
		sliding -= 0.5 * delta
		if Input.is_action_just_pressed(\"Spell 1\") and $\"Spell_1 Cooldown\".time_left <= 0:
			newspell()
			
		if is_on_floor():
			coyote_time = 0.1
		if not is_on_floor():
			velocity += get_gravity() * delta
		
		if $\"Wall Jump Req\".is_colliding():
			$AnimatedSprite2D.play(\"wallsliding\")
			if not Input.is_action_pressed(\"slide\"):
				$AnimatedSprite2D.play(\"wallsliding\")
				if velocity.y > 100:
					velocity.y = 100
			else:
				if velocity.y > 200:
					velocity.y = 200
		# Handle jump.
		if Input.is_action_pressed(\"jump\") and coyote_time > 0 and not $\"Force Sliding Req\".is_colliding():
			velocity.y = JUMP_VELOCITY
			coyote_time = 0
			$AnimatedSprite2D.play(\"jump\")
			sliding = -1
			if abs(velocity.x) < 300:
				velocity.x = 300 * direction
		# Handles Walljump
		if Input.is_action_just_pressed(\"jump\") and $\"Wall Jump Req\".is_colliding() and not is_on_floor():
			velocity.y = JUMP_VELOCITY
			if facing == 1:
				velocity.x = -750
				facing = -1
				$AnimatedSprite2D.flip_h = true
				$\"Wall Jump Req\".position.x = -25
			else: 
				velocity.x = 750
				facing = 1
				$AnimatedSprite2D.flip_h = false
				$\"Wall Jump Req\".position.x = 25

		# Get the input direction and handle the movement/deceleration.
		# As good practice, you should replace UI actions with custom gameplay actions.
		if sliding < 0:
			if direction:
				if velocity.x == 0:
					velocity.x = 1
				if is_on_floor() and (abs(velocity.x) / velocity.x) * direction * abs(velocity.x) < 300 and not Input.is_action_pressed(\"jump\"):
					velocity.x += direction * SPEED * delta
				else:
					if (abs(velocity.x) / velocity.x) * direction * abs(velocity.x) > 300:
						velocity.x *= 0.99
					else:
						if not is_on_floor():
							if (abs(velocity.x) / velocity.x) * direction * abs(velocity.x) < 150:
								velocity.x += direction * 25

			else:
				if is_on_floor():
					velocity.x *= 0.9
				else:
					velocity.x *= 0.95
			if velocity.x > 0:
				turn(1)
			else:
				if velocity.x != 0:
					turn(-1)
		if Input.is_action_just_pressed(\"slide\") and is_on_floor() and not is_on_wall() and sliding < 0:
			sliding = 0.2
			if direction:
				if abs(velocity.x) < 600:
					velocity.x = 600 * direction
				else:
					velocity.x *= 1.5
				$AnimatedSprite2D.play(\"slide\")
				facing = direction
				if direction == 1:
					$AnimatedSprite2D.flip_h = false
				else:
					$AnimatedSprite2D.flip_h = true
			$AnimatedSprite2D.play(\"slide\")
		if $\"Force Sliding Req\".is_colliding() and is_on_floor():
			sliding = 0.01
			
		if sliding > 0:
			if abs(velocity.x) < 600:
				velocity.x = 600 * facing
			$AnimatedSprite2D.play(\"slide\")
			$Hitbox.scale.y = 0.5
			$Hitbox.position.y = 34
		else:
			$Hitbox.scale.y = 1
			$Hitbox.position.y = 5
			
		#The animations
		if Input.is_action_pressed(\"left\") and is_on_floor() and sliding < 0 or Input.is_action_pressed(\"right\") and is_on_floor() and sliding < 0:
			$AnimatedSprite2D.play(\"run\")
		else:
			if sliding > 0:
				$AnimatedSprite2D.play(\"slide\")
			else:
				if is_on_floor():
					$AnimatedSprite2D.play(\"idle\")
				else:
					if not $\"Wall Jump Req\".is_colliding():
						$AnimatedSprite2D.play(\"jump\")

		move_and_slide()


func turn(dir):
	if is_on_floor():
		pass
	else:
		await get_tree().create_timer(0.1).timeout
	if dir == 1:
		$AnimatedSprite2D.flip_h = false
		facing = 1
		$\"Wall Jump Req\".position.x = 25
	else:
		$AnimatedSprite2D.flip_h = true
		facing = -1
		$\"Wall Jump Req\".position.x = -25

func move():
	if is_on_floor():
		velocity.x *= 0.9
	else:
		velocity.x *= 0.95
	if velocity.x > 0:
		turn(1)
	else:
		if velocity.x != 0:
			turn(-1)

func newspell():
	var newSpell = $\"../Spell 1\".duplicate()
	newSpell.initialSpeed = abs(velocity.x)
	newSpell.position = position
	newSpell.facing = facing
	add_sibling(newSpell)
	$\"Spell_1 Cooldown\".start()
	$AttackPlayer.queue(\"attack\")

func damaged(damage):
	if sliding < 0:
		health -= damage
		$AnimationPlayer.queue(\"damaged\")
		var tween = get_tree().create_tween()
		tween.tween_property($\"../../CanvasLayer/HealthBar/HealthBar\", \"scale\", Vector2($\"../../CanvasLayer/HealthBar/HealthBar\".scale - Vector2(max_health / damage * 0.01, 0)), 0.25).set_trans(Tween.TRANS_SINE)
		if health <= 0:
			$AnimationPlayer.play(\"death\")
			$AnimatedSprite2D.play(\"idle\")
			$AnimatedSprite2D.pause()
			$\"../../CanvasLayer/HealthBar/HealthBar\".visible = false
			velocity = Vector2(0, 0)
			state = \"dead\"
			await $AnimationPlayer.animation_finished
			$\"../../CanvasLayer/HealthBar/HealthBar\".visible = true
			health = 100
			position = checkpoint
			await get_tree().create_timer(0.5).timeout
			$AnimationPlayer.play(\"spawn\")
			state = \"alive\"
"

