[sub_resource type="GDScript" id="GDScript_x0ka3"]
resource_name = "Spell 1"
script/source = "extends Area2D

var facing
var initialSpeed
@onready var Sprite = $\"AnimatedSprite2D\"
@onready var Wizard = $\"../Wizard\"
# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	if not facing:
		facing = -1000
		position.y = 1000
	elif facing == -1:
		Sprite.flip_h = true
		$CollisionShape2D.position.x = -10
	else:
		position = Wizard.position + Vector2(10,10)
	Sprite.modulate.a = 0
	get_tree().create_tween().tween_property(Sprite, \"modulate:a\", 1, 0.25)
	await get_tree().create_timer(5).timeout
	if not facing == -1000:
		queue_free()

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	if not facing == -1000:
		move(delta)
		visible = true
	else:
		visible = false

func move(delta):
	position.x += (initialSpeed + 750) * delta * facing


func _on_body_shape_entered(_body_rid: RID, body: Node2D, _body_shape_index: int, _local_shape_index: int) -> void:
		if not body == Wizard and not facing == -1000:
			queue_free()
"

[sub_resource type="GDScript" id="GDScript_bjha6"]
resource_name = "attack 1"
script/source = "extends Area2D

var facing
var initialSpeed
@onready var Sprite = $\"AnimatedSprite2D\"
@onready var Wizard = $\"../Wizard\"

@onready var forestMonster = $\"../Forest Monster\"
# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	$CollisionShape2D.disabled = false
	if not facing:
		facing = -1000
		position.y = 1000
	$AnimatedSprite2D.play(\"Leaf Attack\")
	Sprite.modulate.a = 0
	get_tree().create_tween().tween_property(Sprite, \"modulate:a\", 1, 0.25)

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	if not facing == -1000:
		move(delta)
		visible = true
	else:
		visible = false
		
func move(delta):
	position += initialSpeed * Vector2(delta,delta)
	initialSpeed *= Vector2(0.995, 0.995)
	if abs(initialSpeed.x) + abs(initialSpeed.y) <= 150:
		$CollisionShape2D.disabled = true
		Sprite.speed_scale = 0.5
	if abs(initialSpeed.x) + abs(initialSpeed.y) <= 50:
		Sprite.stop()
		initialSpeed = Vector2.ZERO
	else:
		$CollisionShape2D.disabled = false
		Sprite.speed_scale = 1

func _on_body_shape_entered(_body_rid: RID, body: Node2D, _body_shape_index: int, _local_shape_index: int) -> void:
		if body == Wizard:
			Wizard.damaged(10)
		if not body == forestMonster and not facing == -1000:
			if body == Wizard:
				queue_free()
			else:
				$CollisionShape2D.disabled = true
				initialSpeed = Vector2.ZERO
				Sprite.stop()
				await get_tree().create_timer(15).timeout
				get_tree().create_tween().tween_property(Sprite, \"modulate:a\", 0, 0.25)
"

